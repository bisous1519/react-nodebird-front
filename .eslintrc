{
    "env":{
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        // "airbnb",
        "plugin:prettier/recommended"
    ],
    "settings":{
        "react":{
            "version": "detect"
        }
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins":[
        "import",
        "react-hooks",
        "prettier"
    ],
    "rules":{
        "react/react-in-jsx-scope":0,
        "react/prefer-stateless-function": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-one-expression-per-line": 0,
        "no-nested-ternary": 0
    }
}